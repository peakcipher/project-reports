@article{tavasoli_void_2021,
	title = {Void {Galaxy} {Distribution}: {A} {Challenge} for Λ{CDM}},
	volume = {916},
	issn = {2041-8205, 2041-8213},
	shorttitle = {Void {Galaxy} {Distribution}},
	url = {https://iopscience.iop.org/article/10.3847/2041-8213/ac1357},
	doi = {10.3847/2041-8213/ac1357},
	abstract = {We extract void catalogs from the Sloan Digital Sky Survey Data Release 16 (SDSS DR16) survey and also from the Millennium simulation. We focus our comparison on distribution of galaxies brighter than Mr {\textless} −18 inside voids and study the mean separation of void galaxies, distance from the void center, and the radial density proﬁle. We ﬁnd that mean separation of void galaxies depends on void size, as bigger voids have lower mean separation in both samples. However, void galaxies in the observation sample seem to have generally larger mean–distance than simulated ones at any given void size. In addition, observed void galaxies tend to reside closer to the void center than those in the simulation. This discrepancy is also shown in the density proﬁle of voids. Regardless of the void size, the central densities of real void proﬁles are higher than the ones in the predicted simulated catalog.},
	language = {en},
	number = {2},
	urldate = {2022-09-12},
	journal = {The Astrophysical Journal Letters},
	author = {Tavasoli, Saeed},
	month = aug,
	year = {2021},
	pages = {L24},
	file = {Tavasoli - 2021 - Void Galaxy Distribution A Challenge for ΛCDM.pdf:C\:\\Users\\peakc\\Zotero\\storage\\X42BDFWI\\pdf.pdf:application/pdf},
}
